pipeline {
    agent none

    stages {
        stage('Check Out Code') {
            agent {
                label 'BuildServer'
            }
            steps {
                git credentialsId: 'Saikumar1191_Git_credentials', url: 'https://github.com/saikumar1191/Maven-Java-Project.git'
                
                script {
                    mvnHome = tool name: 'maven3', type: 'maven'
                }
            }
        } 
    }
}
     
